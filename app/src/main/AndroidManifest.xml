            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAXhBdyKxUo_cb-EkSgWJQTdqR0QjLcqes" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Left_page" />
        <activity android:name=".Top_page" />
        <activity android:name=".Bottom_page" />
        <activity android:name=".Add" />
        <activity android:name=".Right_page"></activity>
    </application>

</manifest>